{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "react/jsx-props-no-spreading": [
      "error",
      {
        "custom": "ignore"
      }
    ],
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "operator-linebreak": "off",
    "no-undef": "off"
  }
}
